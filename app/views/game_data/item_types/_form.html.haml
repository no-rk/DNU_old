= render 'game_data/error', { game_data_error: @item_type }
= simple_form_for(@item_type) do |form_item_type|
  = form_item_type.input :name
  = form_item_type.input :caption, :input_html => { :class => :'document-editor span9', :rows => 5 }
  = form_item_type.submit class: "btn btn-large btn-success"
