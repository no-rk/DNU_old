= simple_form_for(register_duel) do |form_duel|
  = register_duel.updated_at.try(:to_s, :jp)
  = render 'register/error', { :register_error => register_duel }
  - unless @read_only
    .actions
      %button.btn.btn-large.btn-warning{"data-remote" => "true"} 確認する
      = form_duel.submit :class => "btn btn-large btn-success"
  - if @update_time
    .actions
      = form_duel.submit :class => "btn btn-large btn-warning"
