= simple_form_for(register_trade) do |form_trade|
  = register_trade.updated_at
  = render 'register/error', { :register_error => register_trade }
  - unless @read_only
    .actions
      %button.btn.btn-large.btn-warning{"data-remote" => "true"} 確認する
      = form_trade.submit :class => "btn btn-large btn-success"
  - if @update_time
    .actions
      = form_trade.submit :class => "btn btn-large btn-warning"
