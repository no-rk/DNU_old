%h1= @register_trades.first.class.model_name.human.downcase
- @register_trades.each do |register_trade|
  .alert
    - if register_trade.day.present?
      %span.badge.badge-important #{register_trade.day.day}æ—¥
    = render 'form', { :register_trade => register_trade }
    = link_to 'Show', register_trade
= paginate @register_trades
